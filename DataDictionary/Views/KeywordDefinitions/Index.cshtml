@model IEnumerable<DataDictionary.Models.KeywordDefinition>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.KeywordDefinitionId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Application)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Field1Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Field2Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Field3Description)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.KeywordDefinitionId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Application.ApplicationId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Field1Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Field2Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Field3Description)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.KeywordDefinitionId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.KeywordDefinitionId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.KeywordDefinitionId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
